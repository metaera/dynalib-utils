{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "clang.cxxflags": [
        "-std=c++17"
    ],
    "testExplorer.showExpandButton": 5,
    "testExplorer.showOnRun": true,
    "testExplorer.errorDecoration": false,
    "testMate.cpp.debug.noThrow": true,
    "testMate.cpp.test.workingDirectory": "${workspaceFolder}",
    "testMate.cpp.test.executables": "{build*}/UnitTests/{Test}*",
    "files.associations": {
        "string": "cpp",
        "cstring": "cpp",
        "ostream": "cpp",
        "iostream": "cpp"
    }
}